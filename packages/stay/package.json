{
  "name": "@nextbnb/service-stay",
  "version": "1.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "update": "rimraf node_modules && yarn",
    "start": "node dist/index.js",
    "clean": "rimraf dist *.tsbuildinfo",
    "build": "tsc -b .",
    "dev": "export AWS_PROFILE=personal && ts-node-dev src/index.ts",
    "test": "jest --passWithNoTests --concurrency 1",
    "lint": "eslint src --ext js,ts,tsx",
    "extract": "api-extractor run --local",
    "deploy": "cd ../../ && yarn docker:stay && docker tag nextbnb-stay:latest 776733965771.dkr.ecr.us-east-1.amazonaws.com/nextbnb-stay:latest && docker push 776733965771.dkr.ecr.us-east-1.amazonaws.com/nextbnb-stay:latest"
  },
  "dependencies": {
    "@nextbnb/common": "file:../common",
    "@nextbnb/database": "file:../database"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.81",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.14.0",
    "jest": "^26.6.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.4"
  },
  "engines": {
    "node": ">= 14.17.1"
  },
  "type": "commonjs"
}
