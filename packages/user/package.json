{
  "name": "@4irbnb/user",
  "version": "1.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "update": "rimraf node_modules && yarn",
    "start": "node dist/index.js",
    "clean": "rimraf dist *.tsbuildinfo",
    "build": "tsc -b .",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --passWithNoTests --detectOpenHandles --watch",
    "lint": "eslint src --ext js,ts,tsx",
    "extract": "api-extractor run --local",
    "deploy": "bash ../../scripts/deploy.sh user api consumer"
  },
  "dependencies": {
    "@4irbnb/common": "file:../common",
    "@4irbnb/manager": "file:../manager",
    "@aws-sdk/client-rds": "^3.32.0",
    "pg": "^8.7.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/pg": "^8.6.1",
    "@types/uuid": "^8.3.1"
  },
  "engines": {
    "node": ">= 14.17.1"
  }
}
